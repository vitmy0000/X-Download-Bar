{
  "name": "My Keymap",
  "version": "1.0",
  "description": "Keymap for quick browser operation",
  "permissions": [
      "<all_urls>",
      "downloads",
      "downloads.shelf"
  ],
  "browser_action": {
      "default_title": "My Keymap",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],
  "background": {
      "scripts": ["background.js"]
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
